{
  "name": "quollabore-playwright-reporter",
  "version": "0.1.0",
  "description": "Playwright reporter for Quollabore portal",
  "type": "module",
  "main": "dist/reporter.cjs",
  "module": "dist/reporter.js",
  "types": "dist/reporter.d.ts",
  "exports": {
